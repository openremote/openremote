{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "type" : "object",
  "properties" : {
    "ref" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "string",
          "pattern" : "^[0-9A-Za-z]{22}$"
        },
        "name" : {
          "type" : "string",
          "minLength" : 1,
          "pattern" : "^\\w+$"
        }
      },
      "required" : [ "name" ],
      "additionalProperties" : true
    },
    "value" : {
      "title" : "Any Type",
      "type" : [ "null", "number", "integer", "boolean", "string", "array", "object" ],
      "additionalProperties" : true
    }
  },
  "title" : "Attribute State",
  "additionalProperties" : true
}