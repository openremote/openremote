{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "title" : "Attribute Ref",
  "type" : "object",
  "additionalProperties" : true,
  "properties" : {
    "id" : {
      "type" : "string",
      "pattern" : "^[0-9A-Za-z]{22}$"
    },
    "name" : {
      "type" : "string",
      "pattern" : "^\\w+$",
      "minLength" : 1
    }
  },
  "required" : [ "name" ]
}