{
  "name": "@openremote/or-icon",
  "version": "1.10.0-snapshot",
  "description": "Icon component for displaying an icon from an icon set",
  "customElements": "custom-elements.json",
  "main": "dist/umd/index.bundle.js",
  "module": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js",
    "./jsx": {
      "types": "./custom-elements-jsx.d.ts"
    }
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "analyze": "npx cem analyze --config ../custom-elements-manifest.config.mjs",
    "copyMdiFont": "node mdi-font-copy.js",
    "build": "npx tsc -b",
    "test": "echo \"No tests\" && exit 0",
    "prepack": "npx rspack"
  },
  "author": "OpenRemote",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@mdi/font": "latest",
    "@openremote/model": "workspace:*",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
