{
  "name": "@openremote/or-translate",
  "version": "1.8.0-snapshot",
  "description": "Provides a web component for translations using i18next",
  "customElements": "custom-elements.json",
  "main": "dist/umd/index.bundle.js",
  "module": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "analyze": "npx cem analyze --config ../custom-elements-manifest.config.mjs",
    "build": "npx tsc -b",
    "test": "npx tsc -b && npx playwright test",
    "prepack": "npx rspack"
  },
  "author": "OpenRemote",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "i18next": "^21.5.3",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@openremote/util": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
