{
  "name": "@openremote/or-json-forms",
  "version": "1.8.0-snapshot",
  "description": "Renders a JSON schema and provides instance editing",
  "customElements": "custom-elements.json",
  "main": "dist/umd/index.bundle.js",
  "module": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "analyze": "npx cem analyze --config ../custom-elements-manifest.config.mjs",
    "build": "npx tsc -b",
    "test": "npx tsc -b && npx playwright test",
    "prepack": "npx rspack"
  },
  "author": "OpenRemote",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@jsonforms/core": "^3.5.1",
    "@openremote/or-components": "workspace:*",
    "@openremote/or-mwc-components": "workspace:*",
    "@polymer/polymer": "^3.3.1",
    "ajv": "^8.8.2",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@openremote/util": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
