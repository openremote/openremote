{
  "name": "@openremote/or-vaadin-input",
  "version": "1.10.0-snapshot",
  "description": "Native input element wrapper that displays Vaadin components",
  "customElements": "custom-elements.json",
  "main": "dist/umd/index.bundle.js",
  "module": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js",
    "./jsx": {
      "types": "./custom-elements-jsx.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  },
  "scripts": {
    "analyze": "npx cem analyze --config ../custom-elements-manifest.config.mjs",
    "build": "npx tsc -b",
    "test": "npx tsc -b && npx playwright test",
    "prepack": "npx tsc -b"
  },
  "author": "OpenRemote",
  "repository": "https://github.com/openremote/openremote",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@openremote/core": "workspace:*",
    "lit": "^3.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
