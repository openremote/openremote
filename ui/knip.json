{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": [
    "**/rspack.config.js",
    "**/typedoc.js",
    "**/playwright.config.ts",
    "ui/app/**/test/**",
    "ui/component/**/test/**",
    "ui/component/custom-elements-manifest.config.mjs",
    "ui/component/or-icon/mdi-iconset-generator.js",
    "ui/component/model/src/index.ts",
    "ui/component/rest/src/index.ts"
  ],
  "ignoreWorkspaces": ["ui/app/swagger", "ui/util", "ui/test"],
  "ignoreDependencies": ["@types/mapbox", "@webcomponents/webcomponentsjs", "@types/offscreencanvas"],
  "rules": {
    "binaries": "off",
    "duplicates": "off"
  },
  "workspaces": {
    ".": {
      "project": ["ui"]
    }
  }
}
