{
  "id": null,
  "uid": "openremote-overview",
  "title": "OpenRemote Overview",
  "tags": ["openremote"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "hideControls": false,
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "refresh": "30s",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "title": "System CPU Usage",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "CPU Usage %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "Memory Usage",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "((node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes) * 100",
          "legendFormat": "Memory Usage %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "title": "Disk Usage",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "((node_filesystem_size_bytes{mountpoint=\"/\"} - node_filesystem_free_bytes{mountpoint=\"/\"}) / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100",
          "legendFormat": "Disk Usage %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
    },
    {
      "id": 4,
      "title": "Active Sessions",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(haproxy_server_current_sessions)",
          "legendFormat": "Active Sessions",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
    },
    {
      "id": 5,
      "title": "HTTP Response Codes",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(haproxy_server_http_responses_total[5m])",
          "legendFormat": "{{code}} responses/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    },
    {
      "id": 6,
      "title": "Network Traffic",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(haproxy_server_bytes_in_total[5m])",
          "legendFormat": "Bytes In/sec",
          "refId": "A"
        },
        {
          "expr": "rate(haproxy_server_bytes_out_total[5m])",
          "legendFormat": "Bytes Out/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "Bps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
    },
    {
      "id": 7,
      "title": "OpenRemote Attribute Events",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(or_attributes_total{source=\"RulesEngine\"}[5m])",
          "legendFormat": "Rules Engine",
          "refId": "A"
        },
        {
          "expr": "rate(or_attributes_total{source=\"AgentService\"}[5m])",
          "legendFormat": "Agents",
          "refId": "B"
        },
        {
          "expr": "rate(or_attributes_total{source=\"DefaultMQTTHandler\"}[5m])",
          "legendFormat": "MQTT",
          "refId": "C"
        },
        {
          "expr": "rate(or_attributes_total{source=\"AssetResource\"}[5m])",
          "legendFormat": "HTTP API",
          "refId": "D"
        },
        {
          "expr": "rate(or_attributes_total{source=\"WebsocketClient\"}[5m])",
          "legendFormat": "WebSocket",
          "refId": "E"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "evt/sec"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
    },
    {
      "id": 8,
      "title": "Rule Execution Performance",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(or_rules_seconds_sum[5m]) / rate(or_rules_seconds_count[5m]) * 1000",
          "legendFormat": "Avg Rule Execution Time (ms)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "ms"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
    },
    {
      "id": 9,
      "title": "Message Queue Backlogs",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "artemis_message_count",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
    },
    {
      "id": 10,
      "title": "Thread Pool Usage",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "executor_pool_size_threads{name=\"ContainerExecutor\"}",
          "legendFormat": "Pool Size",
          "refId": "A"
        },
        {
          "expr": "executor_pool_core_threads{name=\"ContainerExecutor\"}",
          "legendFormat": "Core Threads",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
    }
  ]
}
