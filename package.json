{
  "name": "openremote",
  "packageManager": "yarn@4.1.0",
  "private": true,
  "workspaces": [
    "ui/util",
    "ui/test",
    "ui/app/*",
    "ui/component/*",
    "ui/demo/*"
  ],
  "scripts": {
    "test": "playwright test --config ui/playwright.config.ts",
    "codegen": "playwright codegen http://localhost:9000/manager/"
  },
  "devDependencies": {
    "@openremote/test": "workspace:*",
    "@openremote/util": "workspace:*"
  }
}
